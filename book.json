{
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "disqus",
         "-search",
        "search-pro",
        "github",
        "-sharing",
        "tbfed-pagefooter",
        "toggle-chapters",
        "ga"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "PersonalWi"  
        },
        
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord" : ["Gitbook Use"]
        },
        
        "github": {
            "url": "https://github.com/aleen42"   
        },
        
        "sharing": {
            "weibo": true,
            "facebook": true,
            "twitter": true,
            "google": false,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook", "google", "twitter",
                    "weibo", "instapaper"
            ]
        },
        
        "tbfed-pagefooter": {
            "copyright":"Copyright Â© Soundtooth.inc aleen42 2013-2016",
            "modify_label": "Updated time: ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        
        "ga": {
            "token": "UA-75457732-1"
        }
    }
}