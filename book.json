{
    "structure": {
        "readme": "README.md"
    },
    "styles": {
        "website": "style/website.css",
        "ebook": "style/ebook.css",
        "pdf": "style/pdf.css",
        "mobi": "style/mobi.css",
        "epub": "style/epub.css"
    },
    "links": {
        "sidebar": {
            "GitHub Project": "https://github.com/aleen42/PersonalWiki"
        }
    },
    "plugins": [
        "disqus",
        "-search",
        "github",
        "sharing",
        "ga",
        "customtheme",
        "comments",
        "advanced-emoji",
        "page-footer"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "personal"
        },
        "github": {
            "url": "https://github.com/aleen42"
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "google": true,
            "weibo": true,
            "instapaper": false,
            "vk": false,
            "all": "facebook,google,twitter,weibo,instapaper"
        },
        "ga": {
            "token": "UA-75457732-1"
        },
        "customtheme": {
            "js": [
                "src/index.js"
            ]
        },
        "advanced-emoji": {
            "embedEmojis": true
        },
        "page-footer": {
            "description": "modified at",
            "author": "Aleen",
            "wisdom": "More than a coder, more than a designer",
            "format": "YYYY-MM-DD HH:mm:ss",
            "copyright": "Copyright Â© aleen42",
            "timeColor": "#a",
            "copyrightColor": "#666"
        }
    }
}