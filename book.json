{
    "structure": {
        "readme": "README.md"
    },
    "styles": {
        "website": "style/website.css",
        "ebook": "style/ebook.css",
        "pdf": "style/pdf.css",
        "mobi": "style/mobi.css",
        "epub": "style/epub.css"
    },
    "links": {
        "sidebar": {
            "GitHub Project": "https://github.com/aleen42/PersonalWiki"
        }
    },
    "plugins": [
        "disqus",
        "-search",
        "github",
        "sharing",
        "gitbook-footer",
        "ga",
        "customtheme",
        "comments",
        "advanced-emoji"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "personal"
        },
        "github": {
            "url": "https://github.com/aleen42"
        },
        "sharing": {
            "weibo": true,
            "facebook": true,
            "twitter": true,
            "google": true,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook",
                "google",
                "twitter",
                "weibo",
                "instapaper"
                
            ]
        },
        "gitbook-footer": {
            "description": "modified at",
            "format": "YYYY-MM-DD HH:mm:ss",
            "copyright": "Copyright Â© aleen42",
            "timeColor": "#a10000",
            "copyrightColor": "#666"
        },
        "ga": {
            "token": "UA-75457732-1"
        },
        "customtheme": {
            "js": [
                "src/index.js"
            ]
        },
        "advanced-emoji": {
            "embedEmojis": true
        }
    }
}